<article id="<%= post.layout %>-<%= post.slug %>" class="article article-type-<%= post.layout %>">
    <time datetime="<%= date_xml(post.date) %>">
        {{moment-format date 'YYYY-MM-DD'}}
    </time>
    <h1>
        <a href="/{{path}}">
            {{ title }}
        </a>
    </h1>
    {{{ contents }}}
</article>
